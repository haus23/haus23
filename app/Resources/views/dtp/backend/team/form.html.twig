{% extends 'base.html.twig' %}
{% block title %}Neue Mannschaft - Tipprunde - haus23.net{% endblock %}
{% block body %}
    <h2>Neue Mannschaften</h2>

    {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.shortname) }}
        {{ form_row(form.slug) }}
        <div class="form-group">
            {{ form_widget(form.save) }}
            {{ form_widget(form.saveAndAdd) }}
        </div>
    {{ form_end(form) }}

{% endblock %}
