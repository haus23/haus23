<script src="/assets/bower_components/toastr/toastr.min.js"></script>
<script src="/assets/bower_components/select2/dist/js/select2.min.js"></script>
<script src="/assets/bower_components/select2/dist/js/i18n/de.js"></script>
<!-- endbower -->

<!-- inject:js -->
<script src="/assets/build/js/legacy-app.js"></script>
